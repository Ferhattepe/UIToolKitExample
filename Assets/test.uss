.green-button {
    width: 50%;
    height: 50px;
    -unity-font: url('project://database/Library/unity%20default%20resources?fileID=10102&guid=0000000000000000e000000000000000&type=0#Arial');
    font-size: 20px;
    white-space: nowrap;
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    border-left-width: 4px;
    border-right-width: 4px;
    border-top-width: 4px;
    border-bottom-width: 4px;
    border-left-color: rgb(6, 226, 0);
    border-right-color: rgb(6, 226, 0);
    border-top-color: rgb(6, 226, 0);
    border-bottom-color: rgb(6, 226, 0);
    background-color: rgb(81, 255, 76);
    color: rgb(82, 82, 82);
    -unity-font-style: bold;
}

